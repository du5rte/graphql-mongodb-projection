{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        // "modules": false,
        "corejs": { "version": 3, "proposals": true },
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime"
  ]
}
